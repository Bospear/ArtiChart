{
  "name": "artichart",
  "version": "1.0.0",
  "description": "A React 19 chart library",
  "type": "module",
  "main": "./dist/artichart.umd.js",
  "module": "./dist/artichart.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/artichart.es.js",
      "require": "./dist/artichart.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "react19",
    "chart",
    "charts",
    "visualization",
    "artichart"
  ],
  "author": "Bospear",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3"
  }
}
